{
    "name": "corbosman\/advent2021",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "cash\/lrucache": "^1.0",
        "nunomaduro\/termwind": "^1.2",
        "teewurst\/psr4-advanced-wildcard-composer-plugin": "^3.0",
        "tightenco\/collect": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Lib\\": "lib",
            "day01_calorie_counting\\": "day01_calorie_counting",
            "day02_rock_paper_scissors\\": "day02_rock_paper_scissors",
            "day03_rucksack_reorganization\\": "day03_rucksack_reorganization",
            "day04_camp_cleanup\\": "day04_camp_cleanup",
            "day05_supply_stacks\\": "day05_supply_stacks",
            "day06_tuning_trouble\\": "day06_tuning_trouble",
            "day07_no_space_left_on_device\\": "day07_no_space_left_on_device",
            "day08_treetop_tree_house\\": "day08_treetop_tree_house",
            "day09_rope_bridge\\": "day09_rope_bridge",
            "day10_cathode_ray_tube\\": "day10_cathode_ray_tube"
        },
        "files": [
            "lib\/helpers.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "teewurst\/psr4-advanced-wildcard-composer-plugin": true
        }
    },
    "extra": {
        "teewurst\/psr4-advanced-wildcard-composer-plugin": {
            "autoload": {
                "psr-4": {
                    "%s\\": "{day*}"
                }
            },
            "autoload-dev": {
                "psr-4": {
                    "My\\Namespace\\test\\%s\\": "tests\/{*}\/src"
                }
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "advent"
    ],
    "autoload-dev": {
        "psr-4": []
    }
}